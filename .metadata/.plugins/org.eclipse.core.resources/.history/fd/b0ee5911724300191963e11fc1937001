import java.net.MalformedURLException;

import org.openqa.selenium.By;

import io.appium.java_client.By;
import io.appium.java_client.android.AndroidDriver;
import io.appium.java_client.windows.WindowsElement;

public class BasicAppium1 extends base{

	public static void main(String[] args) throws MalformedURLException  {
		
		@SuppressWarnings("unused")
		AndroidDriver<WindowsElement> driver=Capabilities1();
		driver.findElement(By.className("android.widget.TextView")).click();
		//driver.findElement(MobileBy.AndroidViewTag("android.widget.TextView")).click();
				//AccessibilityId(accessibilityId)By.id("android:id/text1")).click();
	}

}
